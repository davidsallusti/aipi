{
  "name": "docx4js",
  "description": "javascript docx parser",
  "version": "2.0.1",
  "author": {
    "name": "Raymond Li",
    "email": "lalalic@139.com"
  },
  "keywords": [
    "docx",
    "parser",
    "javascript"
  ],
  "license": "GPL",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/lalalic/docx4js.git"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-cli": "^6.6.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "browserify": "^13.0.0",
    "watchify": ">=3.0.0 <4.0.0",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "uglifyify": "^3.0.1",
	"jest":"^14.0.0"
  },
  "dependencies": {
    "jszip": "^2.6.0",
    "xml2js": "^0.4.17",
	"sax":"^1.2.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "babel-plugin-add-module-exports",
      "babel-plugin-transform-object-rest-spread",
      "babel-plugin-transform-class-properties"
    ]
  },
  "main": "./lib/",
  "scripts": {
    "build": "babel src -d lib --source-maps inline",
    "dist.watch": "watchify -v -d -r ./lib/index.js:docx4js -o dist/index.js",
    "dist.min": "browserify -g uglifyify -r ./lib/index.js:docx4js -o dist/docx4js.js",
   
    "karma": "karma start",
	
    "test": "jest",
    "test.http": "http-server dist -c-1",
	
    "doc": "esdoc -c esdoc.json"
  }
}
